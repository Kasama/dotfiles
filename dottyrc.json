{
  "directories": [
    "~/.config"
    , "~/.config/terminator"
    , "~/.config/termite"
    , "~/.config/dunst"
    , "~/.config/kitty"
  ]
  , "link": {
    "prezto": "~/.zprezto"
      , "prezto/runcoms/zlogin": "~/.zlogin"
      , "prezto/runcoms/zlogout": "~/.zlogout"
      , "prezto/runcoms/zpreztorc": "~/.zpreztorc"
      , "prezto/runcoms/zprofile": "~/.zprofile"
      , "prezto/runcoms/zshenv": "~/.zshenv"
      , "prezto/runcoms/zshrc": "~/.zshrc"
    , "nvim": "~/.config/nvim"
    , "tmux": "~/tmux"
      , "./tmux/tmux.conf": "~/.tmux.conf"
    , "gtk-config": "~/.themes"
    , "compiz-config": "~/.config/compiz"
    , "emerald-config": "~/.emerald"
    , "scripts": "~/scripts"
    , "terminator/config": "~/.config/terminator/config"
    , "termite/config": "~/.config/termite/config"
    , "dunst/dunstrc": "~/.config/dunst/dunstrc"
    , "kitty/kitty.conf": "~/.config/kitty/kitty.conf"
    , "git/gitconfig": "~/.gitconfig"
    , "./X/xinitrc": "~/.xinitrc"
    , "./X/xresources": "~/.Xresources"
    , "./X/Xauthority": "~/.Xauthority"
    , "./yaourt/yaourtrc": "~/.yaourtrc"
    , "./i3": "~/.config/i3"
    , "./kwm": "~/.kwm"
    , "./kwm/skhdrc": "~/.skhdrc"
    , "./kwm/chunkwmrc": "~/.chunkwmrc"
    , "./polybar": "~/.config/polybar"
    , "./vimperator": "~/.vimperator"
      , "./vimperator/.vimperatorrc": "~/.vimperatorrc"
    , "./ideavim": "~/.ideavim"
  }
  , "commands": [
    "[[ ! -L ~/nvim ]] && ln -s ~/.config/nvim ~/nvim"
  ]

}
