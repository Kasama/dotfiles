{
  "directories": [
    "~/.config",
    "~/.config/dunst",
    "~/.config/kmonad",
    "~/.config/git",
    "~/.config/bin",
    "~/.config/rofi",
    "~/.config/ghostty",
    "~/.screenlayout"
  ],
  "link": {
    "./kmonad-config/config.kbd": "~/.config/kmonad/config.kbd",
    "prezto": "~/.zprezto",
    "prezto/runcoms/zlogin": "~/.zlogin",
    "prezto/runcoms/zlogout": "~/.zlogout",
    "prezto/runcoms/zpreztorc": "~/.zpreztorc",
    "prezto/runcoms/zprofile": "~/.zprofile",
    "prezto/runcoms/zshenv": "~/.zshenv",
    "prezto/runcoms/zshrc": "~/.zshrc",
    "nvim": "~/.config/nvim",
    "emacs/spacemacs": "~/.spacemacs",
    "emacs/emacs.d": "~/.emacs.d",
    "emacs/emacs.d/.doom.d": "~/.doom.d",
    "tmux": "~/tmux",
    "./tmux/tmux.conf": "~/.tmux.conf",
    "gtk-config": "~/.themes",
    "scripts": "~/scripts",
    "dunst/dunstrc": "~/.config/dunst/dunstrc",
    "git/gitconfig": "~/.gitconfig",
    "git/ignore": "~/.config/git/ignore",
    "./X/xinitrc": "~/.xinitrc",
    "./X/xresources": "~/.Xresources",
    "./X/screenlayout.sh": "~/.screenlayout/default.sh",
    "./X/xmodmap": "~/.Xmodmap",
    "./X/speedswapper": "~/.speedswapper",
    "./yaourt/yaourtrc": "~/.yaourtrc",
    "./i3": "~/.config/i3",
    "./kwm": "~/.kwm",
    "./kwm/skhdrc": "~/.skhdrc",
    "./kwm/chunkwmrc": "~/.chunkwmrc",
    "./polybar": "~/.config/polybar",
    "./rofi": "~/.config/rofi",
    "./vimperator": "~/.vimperator",
    "./vimperator/.vimperatorrc": "~/.vimperatorrc",
    "./ideavim": "~/.ideavim",
    "./ghostty": "~/.config/ghostty",
    "./bin/config.yaml": "~/.config/bin/config.yaml"
  },
  "commands": [
    "[[ ! -L ~/nvim ]] && ln -s ~/.config/nvim ~/nvim",
    "pushd ./kmonad-config && make && popd "
  ]
}
